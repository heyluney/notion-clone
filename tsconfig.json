{
  "compilerOptions": {
    "jsx": "react-jsx",
    "target": "es6", 
    "allowJs": true,                                
    "module": "es6",
    "moduleResolution": "nodenext",
    "esModuleInterop": true,                            
    "forceConsistentCasingInFileNames": true,            
    "strict": true,   
    "skipLibCheck": true                           
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}
